{
  "name": "service",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "npm run build:ts && fastify start --plugin-timeout 30000  -l debug -p 80 -a 0.0.0.0 ./dist/app.js",
    "start:prod": "fastify start --plugin-timeout 30000   -l debug -a 0.0.0.0 -r newrelic ./dist/app.js -- -w auto",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{\"name\"}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --plugin-timeout 30000 --ignore-watch=.ts$ -l debug -P -a 0.0.0.0 -r newrelic ./dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/env": "^5.0.2",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger-ui": "^5.2.3",
    "axios": "^1.11.0",
    "env-schema": "^6.0.1",
    "fastify": "^5.5.0",
    "fastify-cli": "^7.3.0",
    "fastify-plugin": "^5.0.0",
    "openai": "^5.16.0"
  },
  "devDependencies": {
    "@fastify/swagger": "^9.5.1",
    "@types/node": "^24.3.0",
    "c8": "^10.1.2",
    "concurrently": "^9.2.1",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
